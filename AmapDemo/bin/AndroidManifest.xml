<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.amapdemo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="8" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:theme="@style/AppTheme" >
        
        <meta-data
            android:name="com.amap.api.v2.apikey"
            android:value="00b9b56ebbd102fd3ccd323a70ebe845" />
        
        
        <activity 
            android:name=".MainActivity">
            <intent-filter >
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        
        <activity android:name=".basic.BasicActivity"></activity>
        <activity android:name=".basic.HelloActivity"></activity>
        <activity android:name=".basic.CameraActivity"></activity>
        <activity android:name=".basic.EventsActivity"></activity>
        <activity android:name=".basic.LayerActivity"></activity>
        <activity android:name=".basic.MapOptionActivity"></activity>
        <activity android:name=".basic.BaseMapFragmentActivity"></activity>
        <activity android:name=".basic.ScreenShotActivity"></activity>
        <activity android:name=".basic.UISettingActivity"></activity>
        
        <activity android:name=".overlay.PolylineActivity"></activity>
        <activity android:name=".overlay.PolygonActivity"></activity>
        <activity android:name=".overlay.MarkersActivity"></activity>
        <activity android:name=".overlay.GroundOverlayActivity"></activity>
        
        <activity android:name=".location.LocationSourceActivity"></activity>
        <activity android:name=".location.LocationSensorSourceActivity"></activity>
         
        <activity android:name=".poisearch.PoiKeyWordSearchActivity"></activity> 
    </application>

</manifest>
